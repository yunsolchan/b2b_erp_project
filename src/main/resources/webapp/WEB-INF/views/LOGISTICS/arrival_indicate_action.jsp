<body>
<!-- 여기서부터 작성 -->
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<c:if test="${updateCnt == 1}">
	<script type="text/javascript">
	swal("입하지시가 승인되었습니다.", {
	    icon: "success" 
	  })
	  .then((willDelete) => {
	     opener.document.location.reload();
	     self.close();
	  });
	</script>
</c:if>
<c:if test="${updateCnt != 1}">
	<script type="text/javascript">
	swal("입하지시 승인에 실패하였습니다.", {
	    icon: "warning" 
	  })
	  .then((willDelete) => {
	     window.history.back();
	  });
	</script>
</c:if> 
