<script type="text/javascript">

$(document).ready(function(){
	console.log("홈페이지 진입")
	
	 let weatherIcon = {
        '01' : 'fas fa-sun',
        '02' : 'fas fa-cloud-sun',
        '03' : 'fas fa-cloud',
        '04' : 'fas fa-cloud-meatball',
        '09' : 'fas fa-cloud-sun-rain',
        '10' : 'fas fa-cloud-showers-heavy',
        '11' : 'fas fa-poo-storm',
        '13' : 'far fa-snowflake',
        '50' : 'fas fa-smog'
    };
	
    $.ajax({
        url: 'http://api.openweathermap.org/data/2.5/weather?q=Seoul&appid=99b9a9ba127553f2e586b392b9ea71d9&units=metric',
        dataType: 'json',
        type: 'post',
        success: function(data){
        	console.log("날씨 ajax 데이터 : " + data)
            var $Icon = (data.weather[0].icon).substr(0,2);
            var $Temp = Math.floor(data.main.temp) + '&ordm;';
            var $city = data.name;

             $('.CurrIcon').append('<i class="' + weatherIcon[$Icon] + '"></i>');
            /*  $('.CurrIcon').append('http://openweathermap.org/img/wn/'+ $Icon + '@2x.png'); */
             $('.CurrTemp').prepend($Temp);
             $('.City').append($city);
        }
    })
});



</script>
    
