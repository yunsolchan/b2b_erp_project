      <img src="${path}/resources/image/PURCHASE/미청구현황 목록.png" width=1500px/>
      <div style="border: 1px solid black">
         <pre>
            SQL : 
            
            SELECT r.begin_date,
			       c.name client_name,
			       p.product_id,
			       p.name product_name,
			       q.qty,
			       p.cost,
			       a.supply_amount,
			       a.tax_amount amount,
			       a.abstract
			  FROM request r
			  JOIN client c
			    ON r.client_id = c.client_id
			  JOIN request_detail q
			    ON r.request_id = q.request_id
			  JOIN product p
			    ON q.product_id = p.product_id
			  JOIN slip s
			    ON r.request_id = s.request_id
			  JOIN sales_slip a
			    ON s.slip_id = a.slip_id
			 WHERE s.state = '요청'
			   AND a.type = '매입';
         </pre>   
      </div>
