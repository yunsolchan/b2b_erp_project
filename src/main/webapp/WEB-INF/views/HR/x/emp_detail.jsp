<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>인사카드상세</title>
<style type="text/css">
	.wrap { display: flex; justify-content: center; }
	h3 { padding: 50px 0; text-align: center; }
</style>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>

	
	<h3>인사카드상세</h3>
	<div class="wrap">
	
		<div>
			
			<div class="input-group mb-3">
			<div class="input-group-text" id="basic-addon1">사진</div>
			  <input type="image" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1">
			</div>
			
			<div class="input-group mb-3">
			  <div class="input-group-text" id="basic-addon1">부서</div>
			  <div class="input-group-text bg-white form-control" id="basic-addon1">부서</div>
			</div>
			
			<div class="input-group mb-3">
			  <div class="input-group-text" id="basic-addon1">주민번호</div>
			  <div class="input-group-text bg-white form-control" id="basic-addon1">부서</div>
			</div>
			
			<div class="input-group mb-3">
			  <div class="input-group-text" id="basic-addon1">주소</div>
			  <div class="input-group-text bg-white form-control" id="basic-addon1">부서</div>
			</div>

			<div class="input-group mb-3">
			  <span class="input-group-text" id="basic-addon1">주민번호</span>
			  <input type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1" readonly>
			</div>
			
		</div>
		
		&nbsp;&nbsp;&nbsp;&nbsp;
		
		<div>
			<div class="input-group mb-3">
			  <span class="input-group-text" id="basic-addon1">이름</span>
			  <input type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1">
			</div>
			
			<div class="input-group mb-3">
			  <span class="input-group-text" id="basic-addon1">이메일</span>
			  <input type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1">
			</div>

			<div class="input-group mb-3">
			  <span class="input-group-text" id="basic-addon1">전화번호</span>
			  <input type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1">
			</div>
			
			<div class="input-group mb-3">
			  <span class="input-group-text" id="basic-addon1">입사일</span>
			  <input type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1">
			</div>
			
			<div class="input-group mb-3">
			  <span class="input-group-text" id="basic-addon1">직급</span>
			  <input type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1">
			</div>
		</div>
	
	</div>
	
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>	
</body>
</html>